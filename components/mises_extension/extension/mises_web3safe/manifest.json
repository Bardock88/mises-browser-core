{
  "manifest_version": 3,
  "name": "MisesShield",
  "description": "Identify phishing websites and protect your crypto assets.",
  "version": "1.0",

  "icons": {
    "16": "icons/ic_16.png",
    "48": "icons/ic_48.png",
    "128": "icons/ic_128.png"
  },

  "permissions": [
    "storage",
    "background",
    "webRequest"
  ],
  "host_permissions": [
    "https://api.test.mises.site/"
  ],
  "background": {
    "service_worker": "js/background.js"
  },

  "action": {
    "default_icon": "icons/ic_16.png",
    "default_title": "MisesShield"
  },

  "content_scripts": [{
    "matches": ["http://*/*", "https://*/*"],
    "js": ["js/contentScripts.js"],
    "css": ["css/contentScripts.css", "css/contentScriptsWeb3Proxy.css"],
    "run_at": "document_start"
  }],

  "web_accessible_resources": [{
    "resources": [
      "images/*",
      "js/static/*",
      "js/contentScriptsWeb3Proxy.js"
    ],
    "matches": ["<all_urls>"]
  }],
  "key": "",
  "minimum_chrome_version": "88"
}
